<%= form_with model: @lab_report do |f| %>
<% if @lab_report .errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@lab_report .errors.count, "error") %>
      prohibited this lab_report  from being saved:</h2>
    <ul>
      <% lab_report.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
<% end %>
<div style=" padding-left: 700px; ">
<div class="form-group;">
  <%= f.label :title %><br />
  <%= f.text_field :title %>
</div>

<div class="form-group">
  <%= f.label :description %><br />
  <%= f.text_area :description %>
</div>

<div class="actions">
  <%= f.submit :Save %>
  <%= button_to 'Show', @lab_report  %>
</div>
</div>
<% end %>